{
  "typescript": {
    "quotes": "double",
    "semi": true,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "importOrder": [
      "^react",
      "^@/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "jsxSingleQuote": false
  },
  "javascript": {
    "extends": "typescript"
  },
  "typescriptreact": {
    "extends": "typescript"
  },
  "javascriptreact": {
    "extends": "typescript"
  }
}